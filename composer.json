{
    "name": "giift/compare",
    "description": "Compare API versions",
    "authors": [
        {
            "name": "Anisha Humbe",
            "email": "anisha@giift.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:giiftalldev/simplecurl.git"
        },
        {
           "type": "vcs",
           "url": "git@github.com:DimitriGilbert/Reactor.git"
        }
    ],
    "require": {
        "giift/simplecurl": "dev-master",
        "league/plates": "3.*",
        "d2g/reactor": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "phpunit/php-code-coverage": "^4",
        "phpunit/php-token-stream": "1.4.6",
        "phploc/phploc": "*",
        "pdepend/pdepend" : "2.0.4",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer":"2.*",
        "sami/sami": "3.0.*",
        "alecsammon/php-raml-parser": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Giift\\Compare\\": "src/"
        }
    }
}
